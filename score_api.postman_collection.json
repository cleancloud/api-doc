			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "7f8d9931-8575-417a-b2e7-cc83c9df4adf",
						"type": "text/javascript",
						"exec": [
							"var CryptoJS = require(\"crypto-js\");",
							"",
							"authorize({code:pm.variables.get(\"user_token\"),secret:pm.variables.get(\"user_secret\")},pm.request.url.getPath(),pm.request.method,pm.request.body.raw);",
							"",
							"function authorize(credential, resource, method, data) {",
							"    let algorithm = 'SHA-256'",
							"    let signedHeader = 'authorization'",
							"    if (resource.indexOf('?') >= 0) {",
							"        resource = resource.substring(0, resource.indexOf('?'))",
							"    }",
							"    let canonicalRequest = algorithm + '\\n' + ",
							"                           method.toUpperCase() + '\\n' + ",
							"                           resource + '\\n' +",
							"                           '' + '\\n' + ",
							"                           signedHeader + ':\\n' +",
							"                           CryptoJS.enc.Hex.stringify(CryptoJS.SHA256(data)) ",
							"",
							"",
							"    let hashedRequest = CryptoJS.enc.Hex.stringify(CryptoJS.SHA256(canonicalRequest))",
							"    ",
							"    pm.environment.set(\"authorization\", 'Algorithm=' + algorithm +",
							"           ',Credential=' + credential.code +",
							"           ',SignedHeaders=' + signedHeader +",
							"           ',Signature=' + CryptoJS.enc.Hex.stringify(CryptoJS.HmacSHA256(hashedRequest, credential.secret)));",
							"  }",
							""
						]
					}
				},
