						"exec": [
							"var jsonData = JSON.parse(responseBody);",
							"postman.setEnvironmentVariable(\"accessCode\", jsonData.accessCode);",
							"postman.setEnvironmentVariable(\"secretCode\", jsonData.secretCode);"
						]
					}
				}
			]
